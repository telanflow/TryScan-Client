{
    "name": "Tsc",
    "version": "1.0.1",
    "manifest_version": 2,
    "description": "TryScan Chrome插件",
    "permissions": [
        "tabs",
        "cookies",
        "webRequest",
        "webRequestBlocking",
        "http://*/*", 
        "https://*/*", 
        "background"
    ],
    "icons": { 
        "48": "image/icon.png",
        "128": "image/icon128.png"
    },
    "browser_action":{
        "default_icon":"image/icon.png",
        "default_popup":"popup.html"
        },
    "background": {
        "scripts": ["js/background.js"]
    }
}